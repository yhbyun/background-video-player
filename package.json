{
  "name": "background-video-player",
  "version": "0.1.0",
  "author": "YongHun Byun <yonghunbyun@gmail.com>",
  "description": "A electron background video player",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
